<section class="breadcrumb_bg">
   <a href="/"><h2>명지대학교 교수평가</h2></a>
   <!--<ol class="breadcrumb">
      <li><a href="index.html">Home</a></li>
      <li><a href="#">Pages</a></li>
      <li class="active">About Layout 1</li>
   </ol>-->
   <form class="form-inline" method="get" action="search">
     <div class="form-group">
       <label class="sr-only" for="exampleInputEmail3">Email address</label>
       <input type="text" name = "keyword" class="form-control" id="exampleInputEmail3" placeholder="검색">
     </div>
      <button type="submit" class="btn btn-default">검색</button>
   </form>
</section>

<section class="team big_block">
   <div class="container">
      <h2 class="text-center">명지대학교 <strong>교수평가</strong></h2>
      <div class="row">
        <% if @prof_list.size == 0%>
        <h2>검색정보가 없습니다.</h2>
        <% else %>
        <% @prof_list.each do |prof| %>
        <div class="col-md-3 col-sm-6 col-xs-12 block">
            <div>
               <div class="team-social">
                  <ul>
                     <li>
                        <a href="home/<%=prof.id%>/like">
                           <%= image_tag 'like.png' %>좋아요</a></li>
                     <!--<li><a href="#"><i class="fa fa-twitter"></i></a></li>
                     <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                     <li><a href="#"><i class="fa fa-google-plus"></i></a></li>-->
                  </ul>
               </div>
               <img src = <%= prof.imgUrl %> class:'img-responsive' class="size">
            </div>
            
            <div class="details">
               <h3>이 름 : <%= prof.name %></h3>
               <h4>학 과 : <%= prof.dept %></h4>
               <h4>좋아요 : <%= prof.like %></h4>
            </div>
         </div>
         <%end%>
         <%end%>
      </div>
   </div>
</section>